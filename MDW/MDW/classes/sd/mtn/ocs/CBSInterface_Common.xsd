<xs:schema xmlns:common="http://www.huawei.com/bme/cbsinterface/common" xmlns:xs="http://www.w3.org/2001/XMLSchema"
     targetNamespace="http://www.huawei.com/bme/cbsinterface/common" elementFormDefault="qualified"
     attributeFormDefault="unqualified">
    <xs:simpleType name="CustomerID">
        <xs:restriction base="xs:string">
            <xs:maxLength value="23"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CustomerCode">
        <xs:restriction base="xs:string">
            <xs:maxLength value="23"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="IDType">
        <xs:restriction base="xs:integer"/>
    </xs:simpleType>
    <xs:simpleType name="IDNumber">
        <xs:restriction base="xs:string">
            <xs:maxLength value="23"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Gender">
        <xs:restriction base="xs:integer">
            <xs:enumeration value="4050000" id="Male"/>
            <xs:enumeration value="4050001" id="Female"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Date">
        <xs:restriction base="xs:string">
            <xs:maxLength value="8"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DateTime">
        <xs:restriction base="xs:string">
            <xs:maxLength value="14"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Week">
        <xs:restriction base="xs:string">
            <xs:maxLength value="1"/>
            <xs:enumeration value="0" id="Sunday"/>
            <xs:enumeration value="1" id="Monday"/>
            <xs:enumeration value="2" id="Tuesday"/>
            <xs:enumeration value="3" id="Wednesday"/>
            <xs:enumeration value="4" id="Thursday"/>
            <xs:enumeration value="5" id="Friday"/>
            <xs:enumeration value="6" id="Saturday"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Title">
        <xs:restriction base="xs:integer"/>
    </xs:simpleType>
    <xs:simpleType name="CustomerGrade">
        <xs:restriction base="xs:int"/>
    </xs:simpleType>
    <xs:simpleType name="Email">
        <xs:restriction base="xs:string">
            <xs:maxLength value="51"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ZipCode">
        <xs:restriction base="xs:string">
            <xs:maxLength value="8"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Country">
        <xs:restriction base="xs:int"/>
    </xs:simpleType>
    <xs:simpleType name="AreaCode">
        <xs:restriction base="xs:int"/>
    </xs:simpleType>
    <xs:simpleType name="NationType">
        <xs:restriction base="xs:int"/>
    </xs:simpleType>
    <xs:simpleType name="JobType">
        <xs:restriction base="xs:int"/>
    </xs:simpleType>
    <xs:simpleType name="Education">
        <xs:restriction base="xs:int"/>
    </xs:simpleType>
    <xs:simpleType name="MaritalStatus">
        <xs:restriction base="xs:int">
            <xs:enumeration value="0" id="Single"/>
            <xs:enumeration value="1" id="Married"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AccountID">
        <xs:restriction base="xs:string">
            <xs:maxLength value="23"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AccountCode">
        <xs:restriction base="xs:string">
            <xs:maxLength value="23"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PaidMode">
        <xs:restriction base="xs:int">
            <xs:enumeration value="0" id="Prepaid"/>
            <xs:enumeration value="1" id="Postpaid"/>
            <xs:enumeration value="2" id="Hybrid"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="BillFlag">
        <xs:restriction base="xs:integer">
            <xs:enumeration value="0" id="Billable"/>
            <xs:enumeration value="1" id="UnBillable"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PaymentMethod">
        <xs:restriction base="xs:string">
            <xs:maxLength value="8"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Balance">
        <xs:restriction base="xs:long"/>
    </xs:simpleType>
    <xs:simpleType name="SubscriberNo">
        <xs:restriction base="xs:string">
            <xs:maxLength value="63"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SubscriberCode">
        <xs:restriction base="xs:string">
            <xs:maxLength value="23"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="IMSI">
        <xs:restriction base="xs:string">
            <xs:maxLength value="20"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Password">
        <xs:restriction base="xs:string">
            <xs:maxLength value="67"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="BACode">
        <xs:restriction base="xs:string">
            <xs:maxLength value="23"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="OfferId">
        <xs:restriction base="xs:int"/>
    </xs:simpleType>
    <xs:simpleType name="OfferCode">
        <xs:restriction base="xs:string">
            <xs:maxLength value="23"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="OfferOrderKey">
        <xs:restriction base="xs:long"/>
    </xs:simpleType>
    <xs:simpleType name="ProductId">
        <xs:restriction base="xs:int"/>
    </xs:simpleType>
    <xs:simpleType name="ProductCode">
        <xs:restriction base="xs:string">
            <xs:maxLength value="23"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ProductOrderKey">
        <xs:restriction base="xs:long"/>
    </xs:simpleType>
    <xs:simpleType name="ServiceId">
        <xs:restriction base="xs:int"/>
    </xs:simpleType>
    <xs:simpleType name="BrandId">
        <xs:restriction base="xs:int"/>
    </xs:simpleType>
    <xs:simpleType name="FileName">
        <xs:restriction base="xs:string">
            <xs:maxLength value="255"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="BillCycleType">
        <xs:restriction base="xs:int"/>
    </xs:simpleType>
    <xs:simpleType name="NetWorkType">
        <xs:restriction base="xs:int"/>
    </xs:simpleType>
    <xs:simpleType name="OfferType">
        <xs:restriction base="xs:int">
            <xs:enumeration value="0" id="PrimaryOffer"/>
            <xs:enumeration value="1" id="OptionalOffer"/>
            <xs:enumeration value="2" id="CUGOffer"/>
            <xs:enumeration value="3" id="GroupOffer"/>
            <xs:enumeration value="4" id="GroupMemberOffer"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="OfferLevel">
        <xs:restriction base="xs:int">
            <xs:enumeration value="0" id="SubscriberLevel"/>
            <xs:enumeration value="1" id="AccountLevel"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ProductType">
        <xs:restriction base="xs:int">
            <xs:enumeration value="0" id="AppendantServiceProduct"/>
            <xs:enumeration value="1" id="AppendantTariffProduct"/>
            <xs:enumeration value="2" id="IndividualIncentiveProduct"/>
            <xs:enumeration value="3" id="GroupIncentiveProduct"/>
            <xs:enumeration value="20" id="MainProduct"/>
            <xs:enumeration value="21" id="GroupProduct"/>
            <xs:enumeration value="22" id="CUGProduct"/>
            <xs:enumeration value="23" id="GroupMemberProduct"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CallScreenType">
        <xs:restriction base="xs:int">
            <xs:enumeration value="1" id="BlackList"/>
            <xs:enumeration value="2" id="WhiteList"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AccountType">
        <xs:restriction base="xs:string">
            <xs:maxLength value="8"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="HandleChargeFlag">
        <xs:restriction base="xs:int">
            <xs:enumeration value="0" id="HandleChargeFree"/>
            <xs:enumeration value="1" id="HandleChargeDeduct"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="OneTimeCharge">
        <xs:annotation>
            <xs:documentation>该参数仅在CBS方案中有效</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ToCredit" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="OneTimeFee" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="OperationID" type="xs:int" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>如果没有输入OperationID，则取当前业务的业务类型。例如开户业务，对开户的手续费和开户定购产品的手续费，这两种手续费的OperatoinID是不同的</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="SubTradeType" type="xs:int" minOccurs="0"/>
                                    <xs:element name="MainProductKey" type="common:ProductId" minOccurs="0"/>
                                    <xs:element name="AccessMode" type="xs:int" minOccurs="0"/>
                                    <xs:element name="ProductKey" type="common:ProductId" minOccurs="0"/>
                                    <xs:element name="ChargeAmount" type="common:Balance" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>如果没有减免，则不需要填写ChargeAmount。如果全部减免，则不需要传入该笔费用的信息</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="SubscriberNo" type="common:SubscriberNo" minOccurs="0"/>
                                    <xs:element name="AccountCode" type="common:AccountCode" minOccurs="0"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="ToBilling" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>仅在iCBS界面发起的业务中，ToAR参数才有效</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="SubFeeList">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="SubFeeValue" maxOccurs="unbounded">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="AcctId" type="common:AccountID"/>
                                                <xs:element name="SubId" type="common:SubscriberCode"
                                                     minOccurs="0"/>
                                                <xs:element name="ItemList">
                                                    <xs:complexType>
                                                        <xs:sequence>
                                                            <xs:element name="ItemValue" maxOccurs="unbounded">
                                                                <xs:complexType>
                                                                    <xs:sequence>
                                                                        <xs:element name="ItemId">
                                                                            <xs:simpleType>
                                                                                <xs:restriction base="xs:string">
                                                                                    <xs:maxLength value="32"/>
                                                                                </xs:restriction>
                                                                            </xs:simpleType>
                                                                        </xs:element>
                                                                        <xs:element name="Amount"
                                                                             type="common:Balance"/>
                                                                    </xs:sequence>
                                                                </xs:complexType>
                                                            </xs:element>
                                                        </xs:sequence>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="BusiType" type="xs:string" minOccurs="0"/>
                        <xs:element name="Bsno" minOccurs="0">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:maxLength value="20"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="ChannelId" type="xs:string" minOccurs="0"/>
                        <xs:element name="Remark" minOccurs="0">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:maxLength value="128"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="TradeTime" type="common:DateTime" minOccurs="0"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="ToAR" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>仅在iCBS界面发起的业务中，ToAR参数才有效</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Fee2arRequestValue" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="AcctId" type="common:AccountID"/>
                                    <xs:element name="AcctCode" type="common:AccountCode" minOccurs="0"/>
                                    <xs:element name="CustId" type="common:CustomerID"/>
                                    <xs:element name="CustCode" type="common:CustomerCode" minOccurs="0"/>
                                    <xs:element name="SubId" type="common:SubscriberCode" minOccurs="0"/>
                                    <xs:element name="Msisdn" type="common:SubscriberNo" minOccurs="0"/>
                                    <xs:element name="TeleType" minOccurs="0">
                                        <xs:simpleType>
                                            <xs:restriction base="xs:string">
                                                <xs:maxLength value="6"/>
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:element>
                                    <xs:element name="InvoiceDate" type="common:DateTime"/>
                                    <xs:element name="DueDate" type="common:DateTime"/>
                                    <xs:element name="BusiType" type="xs:string"/>
                                    <xs:element name="Bsno">
                                        <xs:simpleType>
                                            <xs:restriction base="xs:string">
                                                <xs:maxLength value="20"/>
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:element>
                                    <xs:element name="isCustomer" type="xs:string"/>
                                    <xs:element name="ChannelId" minOccurs="0">
                                        <xs:simpleType>
                                            <xs:restriction base="xs:string">
                                                <xs:maxLength value="32"/>
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:element>
                                    <xs:element name="Remark" minOccurs="0">
                                        <xs:simpleType>
                                            <xs:restriction base="xs:string">
                                                <xs:maxLength value="128"/>
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:element>
                                    <xs:element name="Fee2arRequestDetailList">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="Fee2arRequestDetailValue" minOccurs="0"
                                                     maxOccurs="unbounded">
                                                    <xs:complexType>
                                                        <xs:all>
                                                            <xs:element name="transType" type="xs:string">
                                                                <xs:annotation>
                                                                    <xs:documentation>预存款：ADV
押金：DEP
欠费:OS</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="productName" type="xs:string"/>
                                                            <xs:element name="orderQty" type="xs:long"/>
                                                            <xs:element name="backQty" type="xs:long"/>
                                                            <xs:element name="salesQty" type="xs:long"/>
                                                            <xs:element name="unitPrice" type="common:Balance"/>
                                                            <xs:element name="charge" type="common:Balance"/>
                                                            <xs:element name="discountAmt" type="common:Balance"/>
                                                            <xs:element name="transDate" type="common:DateTime"/>
                                                            <xs:element name="salesDate" type="common:DateTime"/>
                                                            <xs:element name="feeItemCode" type="xs:string"/>
                                                            <xs:element name="billCycleId" type="xs:string"
                                                                 minOccurs="0"/>
                                                            <xs:element name="taxInclusive" type="xs:string"
                                                                 minOccurs="0"/>
                                                            <xs:element name="glacode" type="xs:string"
                                                                 minOccurs="0"/>
                                                            <xs:element name="waiveGlCode" type="xs:string"
                                                                 minOccurs="0"/>
                                                            <xs:element name="rebateGlcode" type="xs:string"
                                                                 minOccurs="0"/>
                                                            <xs:element name="rebateAmt" type="common:Balance"
                                                                 minOccurs="0"/>
                                                            <xs:element name="resTypeId" type="xs:string"
                                                                 minOccurs="0"/>
                                                            <xs:element name="resModelId" type="xs:string"
                                                                 minOccurs="0"/>
                                                            <xs:element name="resId" type="xs:string"
                                                                 minOccurs="0"/>
                                                            <xs:element name="resCode" type="xs:string"
                                                                 minOccurs="0"/>
                                                            <xs:element name="resAmout" type="common:Balance"
                                                                 minOccurs="0"/>
                                                            <xs:element name="refNo" type="xs:string"
                                                                 minOccurs="0"/>
                                                            <xs:element name="feeType" type="xs:string"
                                                                 minOccurs="0"/>
                                                            <xs:element name="productId" type="xs:string"
                                                                 minOccurs="0"/>
                                                            <xs:element name="spCpId" type="xs:string"
                                                                 minOccurs="0"/>
                                                            <xs:element name="serviceId" type="xs:string"
                                                                 minOccurs="0"/>
                                                            <xs:element name="prmFlag" type="xs:string"
                                                                 minOccurs="0"/>
                                                            <xs:element name="serviceType" type="xs:string"
                                                                 minOccurs="0"/>
                                                            <xs:element name="serviceCategory"
                                                                 type="xs:string" minOccurs="0"/>
                                                            <xs:element name="reasonCode" type="xs:string"
                                                                 minOccurs="0"/>
                                                            <xs:element name="feeSubType" type="xs:string"
                                                                 minOccurs="0"/>
                                                            <xs:element name="invoiceType" type="xs:string"
                                                                 minOccurs="0"/>
                                                            <xs:element name="remark" type="xs:string"
                                                                 minOccurs="0"/>
                                                        </xs:all>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SimpleProperty">
        <xs:sequence>
            <xs:element name="Id">
                <xs:annotation>
                    <xs:documentation/>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="24"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Value">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="255"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SessionEntityType">
        <xs:sequence>
            <xs:element name="Name">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="20"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Password" type="common:Password"/>
            <xs:element name="RemoteAddress">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="23"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="RequestHeader">
        <xs:annotation>
            <xs:documentation>for user information use(add,modify,delete)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="CommandId">
                <xs:annotation>
                    <xs:documentation>(*Message command ID. OCS provide special value according special interface)*)</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="64"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Version">
                <xs:annotation>
                    <xs:documentation>(*Version ID,current version is 1(CRM is always 1)*)</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="24"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="TransactionId">
                <xs:annotation>
                    <xs:documentation>(*Transaction ID,used to compound session, default null (CRM is always null)*)</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="20"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="SequenceId">
                <xs:annotation>
                    <xs:documentation>(*Sequence ID,used to compound session,default 1(CRM is always 1)*)</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="20"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="RequestType">
                <xs:annotation>
                    <xs:documentation>(*valid to compound session, default value is None, enumerate values as follows:
Event, not compound session type
Start, session begin
Continue,session continue
Stop, session stop.
(CRM  is always Event)*)</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="20"/>
                        <xs:enumeration value="Event" id="Event"/>
                        <xs:enumeration value="Start" id="Start"/>
                        <xs:enumeration value="Continue" id="Continue"/>
                        <xs:enumeration value="Stop" id="Stop"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="TenantId" type="xs:integer" minOccurs="0"/>
            <xs:element name="Language" type="xs:string" minOccurs="0"/>
            <xs:element name="SessionEntity" type="common:SessionEntityType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>for security use
it is optional</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InterFrom" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>the source of interface:
4050000 for BSS
4050001 for Bank
4050002 for SP
......
it is optional</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="8"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="InterMode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The mode of interface
4050000 for Face to Face
4050001 for  Website
4050002 for  IVR
......
it is optional</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="8"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="InterMedi" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Interaction Media
if Website then give URL
if IVR then give access number</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="23"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="visitArea" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>visit area
Only IVR is useful
it is optional</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="23"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="currentCell" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The current cell of calling
Only IVR is useful
it is optional</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="23"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="additionInfo" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>addition information
