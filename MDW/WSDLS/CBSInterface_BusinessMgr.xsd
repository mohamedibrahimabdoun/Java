<xs:schema xmlns:business="http://www.huawei.com/bme/cbsinterface/cbs/businessmgr" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.huawei.com/bme/cbsinterface/cbs/businessmgr" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:simpleType name="CustomerGrade">
		<xs:restriction base="xs:string">
			<xs:enumeration value="4050000" id="InvalidClass"/>
			<xs:enumeration value="4050001" id="BronzeClass"/>
			<xs:enumeration value="4050002" id="SilverClass"/>
			<xs:enumeration value="4050003" id="GoldClass"/>
			<xs:enumeration value="4050004" id="PlatinumClass"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Gender">
		<xs:restriction base="xs:string">
			<xs:enumeration value="4050000" id="Male"/>
			<xs:enumeration value="4050001" id="Female"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Paymode">
		<xs:annotation>
			<xs:documentation>Prepaid/Postpaid/Hybrid</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="0" id="Prepaid"/>
			<xs:enumeration value="1" id="Postpaid"/>
			<xs:enumeration value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ValidMode">
		<xs:annotation>
			<xs:documentation>Immediately/Next bill cycle</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="4050000" id="Immediately"/>
			<xs:enumeration value="4050001" id="NextBillCycle"/>
			<xs:enumeration value="4050002" id="NextDay"/>
			<xs:enumeration value="4050003" id="FutureDay"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OperationType">
		<xs:annotation>
			<xs:documentation>Add/Delete/Modify</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1" id="Add"/>
			<xs:enumeration value="2" id="Delete"/>
			<xs:enumeration value="3" id="Modify"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IDType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="4050000"/>
			<xs:enumeration value="4050001"/>
			<xs:enumeration value="4050002"/>
			<xs:enumeration value="4050003"/>
			<xs:enumeration value="4050004"/>
			<xs:enumeration value="4059999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ProductOperationType">
		<xs:annotation>
			<xs:documentation>Add/Delete/Modify/NoChange</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1" id="AddProduct"/>
			<xs:enumeration value="2" id="DeleteProduct"/>
			<xs:enumeration value="3" id="ModifyProduct"/>
			<xs:enumeration value="6" id="ProductNoChange"/>
		</xs:restriction>
	</xs:simpleType>
