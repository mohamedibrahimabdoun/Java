	<xs:complexType name="ProductOrderInfoType">
		<xs:all>
			<xs:element name="ProductID" type="xs:string" nillable="true"/>
			<xs:element name="ProductOrderKey" type="xs:string" nillable="true"/>
			<xs:element name="EffectiveDate" type="xs:string"/>
			<xs:element name="ExpireDate" type="xs:string"/>
			<xs:element name="AutoType" type="xs:int" nillable="true"/>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="BalanceRecordType">
		<xs:all>
			<xs:element name="BalanceDesc" type="xs:string" minOccurs="0"/>
			<xs:element name="Balance" type="xs:long"/>
			<xs:element name="MinMeasureId" type="xs:int" nillable="true" minOccurs="0"/>
			<xs:element name="UnitType" type="xs:int" nillable="true" minOccurs="0"/>
			<xs:element name="AccountType" type="xs:string"/>
			<xs:element name="ExpireTime" type="xs:string"/>
			<xs:element name="AccountCredit" type="xs:long" nillable="true" minOccurs="0"/>
			<xs:element name="AccountKey" type="xs:string" nillable="true" minOccurs="0"/>
			<xs:element name="ProductID" type="xs:string" nillable="true" minOccurs="0"/>
			<xs:element name="ProductOrderKey" type="xs:string" minOccurs="0"/>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="SimpleProperty">
		<xs:sequence>
			<xs:element name="Id" type="xs:string">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
			</xs:element>
			<xs:element name="Value" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ComplexProperty">
		<xs:sequence>
			<xs:element name="Id" type="xs:string"/>
			<xs:element name="SimpleProperty" type="business:SimpleProperty" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Service">
		<xs:sequence>
			<xs:element name="Id" type="xs:string"/>
			<xs:element name="Status" type="xs:string" nillable="true" minOccurs="0"/>
			<xs:element name="RegistrationTime" type="xs:string" minOccurs="0"/>
			<xs:element name="SimpleProperty" type="business:SimpleProperty" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Product">
		<xs:sequence>
			<xs:element name="Id" type="xs:string" nillable="true"/>
			<xs:element name="SimpleProperty" type="business:SimpleProperty" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IndividualCustomer">
		<xs:sequence>
			<xs:element name="Name" type="xs:string"/>
			<xs:element name="Code" type="xs:string" minOccurs="0"/>
			<xs:element name="IdType" type="business:IDType" nillable="true" minOccurs="0"/>
			<xs:element name="IdCode" type="xs:string" minOccurs="0"/>
			<xs:element name="Gender" type="business:Gender" nillable="true"/>
			<xs:element name="Birthday" type="xs:string"/>
			<xs:element name="Address" type="xs:string"/>
			<xs:element name="Grade" type="business:CustomerGrade" nillable="true"/>
			<xs:element name="BelToAreaID" type="xs:string" minOccurs="0"/>
			<xs:element name="Email" type="xs:string" minOccurs="0"/>
			<xs:element name="ZipCode" type="xs:string" minOccurs="0"/>
			<xs:element name="RegistrationTime" type="xs:string" minOccurs="0"/>
			<xs:element name="SimpleProperty" type="business:SimpleProperty" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="CustomerType" type="xs:integer" nillable="true" minOccurs="0"/>
			<xs:element name="Country" type="xs:integer" nillable="true" minOccurs="0"/>
			<xs:element name="NativePlace" type="xs:integer" nillable="true" minOccurs="0"/>
			<xs:element name="NationType" type="xs:integer" nillable="true" minOccurs="0"/>
			<xs:element name="JobType" type="xs:integer" nillable="true" minOccurs="0"/>
			<xs:element name="Education" type="xs:integer" nillable="true" minOccurs="0"/>
			<xs:element name="CreditGrade" type="xs:integer" nillable="true" minOccurs="0"/>
			<xs:element name="CreditAmount" type="xs:integer" nillable="true" minOccurs="0"/>
			<xs:element name="CustomerState" type="xs:integer" nillable="true" minOccurs="0"/>
			<xs:element name="MaritalStatus" type="xs:integer" nillable="true" minOccurs="0"/>
			<xs:element name="Skill" type="xs:string" minOccurs="0"/>
			<xs:element name="SocialNo" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GroupCustomer">
		<xs:sequence>
			<xs:element name="ParentGroupCode" type="xs:string" minOccurs="0"/>
			<xs:element name="GroupCode" type="xs:string"/>
			<xs:element name="IdType" type="business:IDType"/>
			<xs:element name="IdCode" type="xs:string"/>
			<xs:element name="Name" type="xs:string"/>
			<xs:element name="Type" type="xs:string">
				<xs:annotation>
					<xs:documentation>(* Customer Type *)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BelToAreaID" type="xs:string">
				<xs:annotation>
					<xs:documentation>(* Which area customer belongs to *)
</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Customer">
		<xs:choice>
			<xs:element name="Individual" type="business:IndividualCustomer"/>
			<xs:element name="Group" type="business:GroupCustomer"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="SubscriberBasic">
		<xs:sequence>
			<xs:element name="Code" type="xs:string" minOccurs="0"/>
			<xs:element name="BrandId" type="xs:string" nillable="true" minOccurs="0"/>
			<xs:element name="RegistrationTime" type="xs:string" minOccurs="0"/>
			<xs:element name="Lang" type="xs:string" nillable="true"/>
			<xs:element name="SMSLang" type="xs:string" nillable="true" minOccurs="0"/>
			<xs:element name="USSDLang" type="xs:string" nillable="true" minOccurs="0"/>
			<xs:element name="PaidMode" type="business:Paymode" nillable="true"/>
			<xs:element name="InitialCredit" nillable="true" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BelToAreaID" type="xs:string" minOccurs="0"/>
			<xs:element name="MainProductID" type="xs:string" nillable="true"/>
			<xs:element name="SimpleProperty" type="business:SimpleProperty" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Subscriber">
		<xs:complexContent>
			<xs:extension base="business:SubscriberBasic">
				<xs:sequence>
					<xs:element name="IMSI" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Account">
		<xs:sequence>
			<xs:element name="Name" type="xs:string"/>
			<xs:element name="Code" type="xs:string" minOccurs="0"/>
			<xs:element name="Address" type="xs:string"/>
			<xs:element name="Title" type="xs:string"/>
			<xs:element name="BillcycleType" type="xs:string" nillable="true"/>
			<xs:element name="RelationCustomer" type="xs:int" minOccurs="0"/>
			<xs:element name="SimpleProperty" type="business:SimpleProperty" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BatchCommon">
		<xs:sequence/>
	</xs:complexType>
	<xs:complexType name="BindSubscriberNo">
		<xs:all>
			<xs:element name="SubscriberNo" type="xs:string"/>
			<xs:element name="SubscriberNoType" type="xs:int"/>
			<xs:element name="IMSI" type="xs:string" minOccurs="0"/>
			<xs:element name="ApplyTime" type="xs:string" minOccurs="0"/>
			<xs:element name="ExpireTime" type="xs:string" minOccurs="0"/>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="Common">
		<xs:sequence>
			<xs:element name="SubscriberNo" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>The following is single operation.</xs:documentation>
	</xs:annotation>
	<xs:complexType name="QueryBasicInfoRequest">
		<xs:sequence>
			<xs:element name="SubscriberNo" type="xs:string"/>
			<xs:element name="QueryType">
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
						<xs:enumeration value="3"/>
						<xs:enumeration value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CustID" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="QueryBasicInfoResult">
		<xs:sequence>
			<xs:element name="Customer" minOccurs="0">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="business:IndividualCustomer"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Subscriber" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Subscriber">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="business:Subscriber"/>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="Product" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="business:Product">
										<xs:sequence>
											<xs:element name="ProductOrderKey" type="xs:string" nillable="true" minOccurs="0"/>
											<xs:element name="EffectiveDate" type="xs:string"/>
											<xs:element name="ExpiredDate" type="xs:string"/>
											<xs:element name="status" type="xs:int" nillable="true"/>
											<xs:element name="CurCycleStartTime" type="xs:string" minOccurs="0"/>
											<xs:element name="CurCycleEndTime" type="xs:string" minOccurs="0"/>
											<xs:element name="BillStatus" type="xs:int" minOccurs="0"/>
											<xs:element name="BindSubscriberNo" type="business:BindSubscriberNo" minOccurs="0" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="Service" type="business:Service" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="SubscriberAccount" minOccurs="0">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="business:Account">
							<xs:sequence>
								<xs:element name="SubscriberNo" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Account" maxOccurs="unbounded">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="business:Account">
							<xs:sequence>
								<xs:element name="SubscriberNo" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CheckPasswordRequest">
		<xs:sequence>
			<xs:element name="SubscriberNo" type="xs:string">
				<xs:annotation>
					<xs:documentation>(*MSISDN*)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Password" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="NewSubscriberRequest">
		<xs:sequence>
			<xs:element name="SubscriberNo" type="xs:string"/>
			<xs:element name="Customer" type="business:IndividualCustomer" minOccurs="0"/>
			<xs:element name="Subscriber">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="business:Subscriber">
							<xs:sequence>
								<xs:element name="Password" type="xs:string" minOccurs="0"/>
								<xs:element name="SCPID" type="xs:int" minOccurs="0"/>
								<xs:element name="CBPID" type="xs:int" minOccurs="0"/>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Account" type="business:Account" minOccurs="0"/>
			<xs:element name="Product" type="business:Product" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="NewSubscriberResult">
		<xs:sequence>
			<xs:element name="ProductOrderInfo" type="business:ProductOrderInfoType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ReNewSubscriberRequest">
		<xs:sequence>
			<xs:element name="SubscriberNo" type="xs:string"/>
			<xs:element name="Subscriber">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="business:Subscriber">
							<xs:sequence>
								<xs:element name="SubscriberNo" type="xs:string"/>
								<xs:element name="Password" type="xs:string" minOccurs="0"/>
								<xs:element name="SCPID" type="xs:int" minOccurs="0"/>
								<xs:element name="CBPID" type="xs:int" minOccurs="0"/>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Account" type="business:Account" minOccurs="0"/>
			<xs:element name="Product" type="business:Product" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="CustID" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
