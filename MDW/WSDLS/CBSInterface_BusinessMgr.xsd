			<xs:documentation>3000~3999</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:maxInclusive value="3000"/>
			<xs:maxExclusive value="3999"/>
		</xs:restriction>
	</xs:simpleType>
